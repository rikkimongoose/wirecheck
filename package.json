{
  "name": "wirecheck",
  "version": "1.0.0",
  "description": "Mock Test Generator with NestJS and Mongoose",
  "author": "Rikki T. T. Mongoose",
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "build": "tsc -p tsconfig.build.json",
    "seed": "nest start --entryFile seed/workflow --watch=false",
    "lint": "eslint '{src,tests}/**/*.ts' --fix",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "mongoose": "^7.6.0",
    "joi": "^17.9.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.0"
  },
  "overrides": {
    "glob": "^10.3.0"
  }
}
